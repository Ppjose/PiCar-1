[Unit]
Description=Webserver for Flask
After=network.target

[Service]
User=root
ExecStart=/usr/local/bin/gunicorn -w 4 -b 0.0.0.0:80 --pythonpath /home/vendetta/PiCar webserver.main:app
Environment=LANG=en_US.UTF-8,LC_ALL=en_US.UTF-8

[Install]
WantedBy=multi-user.target